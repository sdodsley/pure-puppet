{
  "name": "pure-puppet",
  "version": "0.0.1",
  "author": "",
  "summary": "Module for managing a Pure Storage FlashArray.",
  "license": "Apache-2.0",
  "source": "https://github.com/purestorage",
  "project_page": "https://github.com/purestorage",
  "issues_url": "https://github.com/purestorage",
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 3.7.0"
    }
